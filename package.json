  "scripts": {
    "dev": "keystone dev",
    "start": "keystone start",
    "devstart": "cross-env SESSION_SECRET='---DEV-COOKIE-SECRET--CHANGE-ME---' keystone start",
    "build": "keystone build && yarn migrate:deploy",
